{
  "name": "lpf-frontend-cra",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "axios": "0.16.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "2.2.5",
    "globalize": "1.3.0",
    "lint": "^1.1.2",
    "lodash": "4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "moment": "2.18.1",
    "node-sass-chokidar": "^0.0.3",
    "normalizr": "^3.2.4",
    "npm-run-all": "^4.1.2",
    "object-assign": "4.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-big-calendar": "0.15.0",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^16.1.0",
    "react-file-input-previews-base64": "^1.0.6",
    "react-keydown": "^1.9.4",
    "react-redux": "5.0.5",
    "react-render-html": "0.5.0",
    "react-router-dom": "^4.2.2",
    "react-rte": "^0.15.0",
    "react-scripts": "1.0.17",
    "redux": "3.6.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "2.1.0",
    "sanitize-html": "^1.15.0",
    "semantic-ui-css": "2.2.12",
    "semantic-ui-react": "0.71.5",
    "toastr": "2.1.2"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "eject" : "react-scripts eject",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; npm-run-all build-css build-js'",
    "build:test": "REACT_APP_ENV=test npm run build",
    "build:production": "REACT_APP_ENV=production npm run build",
    "build:windows-test": "env-cmd .env.test npm-run-all build-css build-js",
    "build:windows-production": "env-cmd .env.production npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "coverage": "yarn test --coverage && open ./coverage/lcov-report/index.html",
    "lint": "./node_modules/.bin/eslint ./src/*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "deep-freeze": "^0.0.1",
    "env-cmd": "^7.0.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "prettier": "1.8.2",
    "react-test-renderer": "^16.1.0"
  }
}
